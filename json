function getTemp(){
  try{
    
   var dat="ap"
  switch(dat){
    case "ap": my(dat)
            Logger.log(dat)
            break
    case "bihar":my(dat)
    Logger.log("bihar")
    break
    
    case "Vizag": my(dat)
            Logger.log("Vizag")
            break
          
    case "Kakinada": my(dat)
                Logger.log("kakinada")
                break
  }
  }
catch (error){
Logger.log(error)
}

}
function my(getData) {
  var data =   {"ap":30,"bihar":40,"Kakinada":34}
  data=JSON.stringify(data)
  var parsedData = JSON.parse(data);

  Logger.log(parsedData[getData])
}
